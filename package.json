{
  "name": "elevenlabs-voice-agent-mcp",
  "version": "1.1.0",
  "description": "MCP server for ElevenLabs Voice Agent Development",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "elevenlabs-voice-agent-mcp": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "keywords": [
    "mcp",
    "model-context-protocol",
    "elevenlabs",
    "voice-agent",
    "ai",
    "claude",
    "voice",
    "tts",
    "conversational-ai",
    "outbound-calling",
    "twilio"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/griffinwork40/elevenlabs-voice-agent-mcp.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc && chmod +x dist/index.js",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:types": "tsc --noEmit -p tsconfig.test.json",
    "test:integration": "npm run test:startup && npm run test:api && npm run test:mcp && npm run test:tools",
    "test:startup": "tsx test/test-server-startup.ts",
    "test:api": "tsx test/test-api-connection.ts",
    "test:mcp": "tsx test/mcp-client-test.ts",
    "test:tools": "tsx test/test-tools.ts",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1",
    "axios": "^1.7.9",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@vitest/coverage-v8": "^4.0.16",
    "dotenv": "^17.2.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  }
}
